{
  "name": "writsy",
  "description": "Premium WordPress theme by Incredibbble",
  "main": "index.php",
  "license": "ISC",
  "moduleType": [],
  "homepage": "",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "font-awesome": "fontawesome#^4.6.3",
    "lazysizes": "^2.0.2",
    "matchHeight": "matchheight#^0.7.0",
    "slick-carousel": "^1.6.0",
    "simplelightbox": "^1.8.3",
    "hammerjs": "^2.0.8"
  },
  "overrides": {
    "font-awesome": {
      "main": [
        "css/*",
        "fonts/*"
      ]
    },
    "slick-carousel": {
      "main": [
        "slick/slick.css",
        "slick/slick.js",
        "slick/slick.min.js"
      ],
      "dependencies": null
    },
    "matchHeight": {
      "main": "dist/*"
    },
    "lazysizes": {
      "main": [
        "lazysizes.js",
        "lazysizes.min.js",
        "plugins/**/*"
      ]
    },
    "hammerjs": {
      "main": [
        "hammer.js",
        "hammer.min.js"
      ]
    }
  }
}
